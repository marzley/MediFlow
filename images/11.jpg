    <script type="6352b907e353f73be0ffacfa-text/javascript">
        function show_time() {
            $('.daojishi').addClass('on').show();  
            var timer = setInterval(function(){         
                $(".free-download-limit .countdown .timer").each(function(){   
                    var Obj = this;        
                    var EndTime = new Date('1970/01/01 08:00');      
                    var NowTime = new Date();  
                    if(EndTime<NowTime){ 
                        let eH = NowTime.getHours() + 1
                        if(NowTime.getMinutes()<= EndTime.getMinutes()){
                            eH = NowTime.getHours()
                        }
                        EndTime = new Date( new Date().setHours(eH, EndTime.getMinutes(), 0 ,0)); 
                    }
                    var nMS=EndTime.getTime() - NowTime.getTime();    
                    var nD=Math.floor(nMS/(1000 * 60 * 60 * 24));    
                    var nH=Math.floor(nMS/(1000*60*60)) % 24;    
                    var nM=Math.floor(nMS/(1000*60)) % 60;    
                    var nS=Math.floor(nMS/1000) % 60;    
                    var nMS=Math.floor(nMS/10) % 100;
                    if(nD<10){
                        nD ="0"+nD;
                    }   
                    if(nH<10){
                        nH ="0"+nH;
                    }  
                    if(nM<10){
                        nM = "0"+nM;
                    }
                    if(nS<10){
                        nS = "0"+nS;
                    }
                    if( nMS >0 && nMS<10){
                        nMS = "0"+nMS;
                    }
                    if(nMS<0){
                        clearInterval(timer);
                    } else {
                        var str = '<span>' + nH + '</span>' + ' : ' + '<span>' + nM + '</span>' + ' : ' + '<span>' + nS + '</span>' + ' : ' + '<span>' + nMS + '</span>';
                        $(Obj).html(str);
                    }
                });
            }, 10);
        } 
        show_time()

