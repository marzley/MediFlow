                                            isLoaded = true;
                                        }
                                    }
                                }
                                if (isLoaded) {
                                    setTimeout(function () {
                                        callback(null, node);
                                    }, 1);
                                } else {
                                    setTimeout(function () {
                                        poll(node, callback);
                                    }, 10);
