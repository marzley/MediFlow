                            function poll(node, callback) {
                                var isLoaded = false;
                                if (/webkit/i.test(navigator.userAgent)) {
                                    //webkit
                                    if (node['sheet']) {
                                        isLoaded = true;
                                    }
                                } else if (node['sheet']) {
